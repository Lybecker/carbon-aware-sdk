      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DataSources": {
    "EmissionsDataSource": "test",
    "ForecastDataSource": "WattTime",
    "Configurations": {
      "test": {
        "Type": "JSON",
        "DataFileLocation": "test-data-file.json"
      },
      "WattTime": {
